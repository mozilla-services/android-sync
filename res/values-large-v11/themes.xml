<!-- themes.xml is present in mobile/android/base/resources/values and
     is not owned by Android Sync.  We include this just so we can
     build the Android Sync APK without depending on Fennec.  See also
     drawable/icon.png. -->

<resources>

    <!--
        Base application theme. This could be overridden by GeckoBaseTheme
        in other res/values-XXX/themes.xml.
    -->
    <style name="GeckoBase" parent="@android:style/Theme.Holo.Light">
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:windowNoTitle">true</item>
    </style>
    
</resources>
